{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development node ./src/app.js",
    "start_win": "set NODE_ENV=development&& node ./src/app.js",
    "start_production": "set NODE_ENV=production&& node ./src/app.js",
    "test_unit": "mocha",
    "coverage": "c8 --all --reporter=cobertura --reporter=text-summary npm run test_unit"
  },
  "mocha": {
    "exit": true,
    "recursive": true,
    "spec": [
      "./unit-tests/services/"
    ],
    "reporter": "cypress-multi-reporters",
    "reporter-options": "configFile=mocha-reporters-conf.json"
  },
  "dependencies": {
    "cookie-session": "^2.1.0",
    "cors": "^2.8.5",
    "express": "~5.1.0",
    "mongodb": "^6.17.0",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "c8": "^10.1.3",
    "chai": "^4.5.0",
    "chai-as-promised": "7.1.2",
    "chai-exclude": "2.1.1",
    "cypress-multi-reporters": "^2.0.5",
    "mocha": "^11.7.5",
    "mocha-junit-reporter": "^2.2.1",
    "mongo-mock": "^4.2.0",
    "sinon": "^21.0.1"
  }
}
